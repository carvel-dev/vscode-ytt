{
    "name": "vscode-ytt",
    "displayName": "vscode-ytt",
    "description": "Syntax highlighting for ytt",
    "version": "0.0.3",
    "publisher": "ewrenn",
    "repository": {
        "type": "git",
        "url": "https://github.com/k14s/vscode-ytt"
    },
    "bugs": {
        "url": "https://github.com/k14s/vscode-ytt/issues"
    },
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ytt",
                "extensions": [
                    ".yaml",
                    ".yml"
                ],
                "aliases": [
                    "ytt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ytt",
                "scopeName": "source.yaml.ytt",
                "path": "./syntaxes/ytt.tmLanguage.json"
            }
        ]
    }
}
